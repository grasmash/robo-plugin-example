{
    "name": "grasmash/robo-plugin-example",
    "description": "An example of a Robo plugin.",
    "type": "library",
    "license": "MIT",
    "repositories": {
        "robo-plugin-manager": {
            "type": "path",
            "url": "../robo_plugin_manager",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "grasmash/robo-plugin-manager": "*@dev"
        },
    "autoload": {
        "psr-4": {
            "Robo\\Plugin\\": "src/"
        }
    },
    "extra": {
        "robo": {
            "operations": {
                "install": "\\RoboPluginExample\\Robo\\Plugin\\Plugin::install"
            }
        }
    }
}
